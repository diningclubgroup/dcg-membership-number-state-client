{
  "name": "dcg/dcg-membership-number-state-client",
  "description": "Client for DCG Membership Number State Service",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@bitbucket.org:tastecard/dcg-lib-config.git"
    }
  ],
  "require": {
    "php": ">=5.4"
    "guzzlehttp/guzzle": "^5.3",
    "dcg/dcg-lib-config": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.8"
  },
  "autoload": {
    "psr-4": {
      "Dcg\\Client\\MembershipNumberState\\Utils\\":"src/Utils",
      "Dcg\\Client\\MembershipNumberState\\": "src/MembershipNumberStateClient"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dcg\\Client\\MembershipNumberState\\Tests\\": "tests/"
    }
  }
}

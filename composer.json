{
  "name": "dcg/dcg-lib-membership-number-state-client",
  "description": "Client for DCG Membership Number State Service",
  "type": "library",
  "require": {
    "php": "^5.5|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.8",
    "guzzlehttp/guzzle": "~5.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
  },
  "autoload": {
    "psr-4": {
      "Dcg\\Client\\MembershipNumberState\\": "src/MembershipNumberStateClient",
      "Dcg\\Client\\MembershipNumberState\\Utils\\":"src/Utils"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dcg\\Client\\MembershipNumberState\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "Dcg\\Client\\MembershipNumberState\\Config\\FileCreator::createConfigFile"
    ]
  }
}
